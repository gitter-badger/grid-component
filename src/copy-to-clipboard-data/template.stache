<div class="clickable"></div>

{{#if modalVisible}}
<ctc-modal modal-visible="{modalVisible}" copy-to-clipboard-tag-name="copy-to-clipboard-data">
  <table>
    <tbody>
      {{#each data}}
      <tr>
        {{#each .}}
        <td>{{.}}</td>
        {{/each}}
      </tr>
      {{/each}}
    </tbody>
  </table>
</ctc-modal>
{{/if modalVisible}}